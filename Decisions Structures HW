average_speed = float(input("Enter your average speed (mph): "))
speed_limit = float(input("Enter the speed limit (mph): "))
distance = float(input("Enter the distance traveled (miles): "))

time_at_limit = distance / speed_limit
time_at_average = distance / average_speed


time_saved = time_at_limit - time_at_average
minutes_saved = time_saved * 60

print(f'You saved {minutes_saved:.2f} miuntes.')

